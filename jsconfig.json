{
	"compilerOptions": {
		"baseUrl": "./server",
		"paths": {
			"#data/*": ["data/*"],
			"#config/*": ["config/*"],
			"#models/*": ["models/*"],
			"#routes/*": ["routes/*"],
			"#middlewares/*": ["middlewares/*"],
		},
	},
	"include": ["server/**/*"],
	"exclude": ["node_modules", "**/node_modules/*"]
}
